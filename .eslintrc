{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "react-app",
        "react-app/jest",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react-hooks/exhaustive-deps": "off",
        "prettier/prettier": ["error"]
    }
}
